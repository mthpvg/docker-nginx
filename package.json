{
  "name": "docker-static-nginx",
  "version": "1.0.0",
  "description": "Serves static files from a nginx Docker's container",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "docker build -t static-nginx .",
    "stop": "docker stop static-nginx",
    "remove": "docker rm static-nginx",
    "start": "docker run -d -p 80:80 --name=static-nginx static-nginx"
  },
